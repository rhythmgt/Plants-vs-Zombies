<AnchorPane fx:id="myPane" prefHeight="720.0" prefWidth="1280.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.homeController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" layoutX="-1.0">
